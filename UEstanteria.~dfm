  object IBQr_Secciones: TIBQuery
    Database = DTM_DATOS.IDB_BaseDatos
    Transaction = DTM_DATOS.IBT_1
    AfterOpen = IBQr_SeccionesAfterOpen
    AfterScroll = IBQr_SeccionesAfterScroll
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select idseccion, cod_stock, seccion, cap_max'
      'from secciones_estanteria'
      'where cod_stock = :p0'
      'order by seccion asc;')
