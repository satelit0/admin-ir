  object IBQr_Estates: TIBQuery
    Database = DTM_DATOS.IDB_BaseDatos
    Transaction = DTM_DATOS.IBT_1
    AfterOpen = IBQr_EstatesAfterOpen
    AfterScroll = IBQr_EstatesAfterScroll
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select cod_stock, nombre_estante, descripcion_estante'
      'from estanteria'
      'where nombre_estante like :p0'
      'order by nombre_estante asc;')
