  object IBQr_Diagnostico: TIBQuery
    Database = DTM_DATOS.IDB_BaseDatos
    Transaction = DTM_DATOS.IBT_1
    AfterOpen = IBQr_DiagnosticoAfterOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select FECHA_DIAGNOSTICO, ID_MEDICO, IDPACIENTE, IDDIAGNOSTICOPA' +
        'CIENTE, CODIGO_PACIENTE, NOMBRES ||'#39' '#39'|| APELLIDOS as nombrec, S' +
        'EXO'
      ''
      'from DIAGNOSTICOS_PACIENTES,  PACIENTES, MEDICOS '
      ''
      'where (PACIENTES.ESTADO = 1) and (ID_PACIENTE = IDPACIENTE) and '
      ''
      '(ID_MEDICO = IDMEDICO) and (ID_MEDICO = :p0) and '
      '(FECHA_DIAGNOSTICO = :p1) order by CODIGO_PACIENTE ;')
    Left = 64
    Top = 377
    ParamData = <
